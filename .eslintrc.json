{
  "extends": "airbnb",
  "env": {
    "node": true,
    "browser": true
  },
  "installedESLint": true,
  "plugins": [
    "react",
    "jsx-a11y",
    "import"
  ],
  "rules": {
    "react/prop-types": 0,
    "semi": [1, "always"],
    "comma-dangle": [0, "never"],
    "no-underscore-dangle": 0,
    "no-console": 0
  }

}
